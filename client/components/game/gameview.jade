#navigation.col-md-12
    .back
    .reset
    .play
#level.col-md-10
    .level-inner
        dim-grid-directive
#instructions.col-md-2
    .instructions-inner
        ul
          li(ng-repeat='ins in vm.instructions') 
            img(id='{{ins.name}}' ng-src='{{ins.src}}' ng-click='vm.addToProgram(ins)' data-drag='true' index='{{$index}}' jqyoui-draggable="{onStart:'vm.replace', onStop:'vm.removeElem'}")
#program.col-md-10
    .program-inner(data-drop="true" jqyoui-droppable="{onDrop:'vm.addToProgram'}")
        ul
            li(ng-repeat='ins in vm.program track by $index') 
                img(class='{{ins.name}}' ng-src='{{ins.src}}' ng-click='vm.removeFromProgram($index)')
                
